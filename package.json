{
  "name": "daily-bread",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "engines": {
    "node": "7.9.0"
  },
  "scripts": {
    "dev": "next",
    "deploy": "concurrently \"npm run deploy:server\" \"npm run deploy:build\" \"npm run deploy:commit\" \"npm run deploy:push\"",
    "deploy:server": "cross-env PORT=3000 npm start",
    "deploy:build": "npm run build",
    "deploy:commit": "git add . && git commit - \"moar bread, regen site\"",
    "deploy:push": "git subtree push --prefix out origin gh-pages",
    "build": "next build && next export",
    "start": "node server -p $PORT",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "jaketrent",
  "license": "MIT",
  "dependencies": {
    "express": "^4.15.3",
    "front-matter": "^2.1.2",
    "next": "^3.0.0-beta6",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-markdown": "^2.5.0"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "cross-env": "^5.0.0"
  }
}
